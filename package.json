{
  "name": "@pomgui/database-pg",
  "version": "1.0.4",
  "description": "Library to handle PostgreSQL database connections allowing named parameters",
  "keywords": [
    "database",
    "named",
    "parameters",
    "camelCase"
  ],
  "author": "Wilfredo Pomier (wpomier@pomgui.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pomgui/database-pg.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./coverage ./dist",
    "clean:all": "rm -rf ./coverage ./dist ./node_modules",
    "cover": "nyc npm run test",
    "lint": "eslint . --ext .ts --fix",
    "preversion": "npm run lint && npm run test",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "test": "jest -i",
    "test:cover": "jest -i --coverage",
    "upgrade": "npx npm-check -u"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "@types/pg": "^8.6.6"
  },
  "dependencies": {
    "@pomgui/database": "^1.1.0",
    "pg": "^8.8.0",
    "sitka": "^1.0.5",
    "tslib": "^2.4.1"
  }
}
